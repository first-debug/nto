<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="650.0" prefWidth="1240.0" spacing="20.0"
      xmlns="http://javafx.com/javafx">
    <MenuBar fx:id="menuBar" prefHeight="17.0" prefWidth="552.0">
        <Menu mnemonicParsing="false" text="Файл">
            <MenuItem mnemonicParsing="false" onAction="#exit" text="Выход"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Помощь">
            <MenuItem mnemonicParsing="false" onAction="#showDocumentation" text="О приложении"/>
        </Menu>
    </MenuBar>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Добавить помещение">
        <font>
            <Font size="30.0"/>
        </font>
    </Text>
    <HBox alignment="CENTER" prefHeight="452.0" prefWidth="1080.0" spacing="20.0">
        <VBox prefHeight="452.0" prefWidth="425.0" spacing="10.0">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0"/>
            </padding>
            <HBox prefHeight="31.0" prefWidth="428.0">
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Название" wrappingWidth="235.611328125">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Text>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Тип" wrappingWidth="42.2783203125">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Text>
                <ChoiceBox fx:id="typeInput"/>
            </HBox>
            <VBox prefHeight="42.0" prefWidth="402.0">
                <TextField fx:id="titleInput" onMouseClicked="#hideWarnings" prefHeight="25.0" prefWidth="252.0"
                           promptText="Название">
                    <cursor>
                        <Cursor fx:constant="TEXT"/>
                    </cursor>
                    <VBox.margin>
                        <Insets right="180.0"/>
                    </VBox.margin>
                </TextField>
                <Text fx:id="warningTitle" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: red;"
                      text="Нужно ввести название!" visible="false"/>
            </VBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Описание" wrappingWidth="177.45947265625">
                <font>
                    <Font size="18.0"/>
                </font>
            </Text>
            <VBox prefHeight="101.0" prefWidth="415.0">
                <TextArea fx:id="descriptionInput" onMouseClicked="#hideWarnings" prefHeight="103.0" prefWidth="415.0"
                          promptText="Описание" wrapText="true"/>
                <Text id="warning-text" fx:id="warningDescription" strokeType="OUTSIDE" strokeWidth="0.0"
                      style="-fx-fill: red;" text="Нужно ввести описание!" visible="false"/>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                <HBox alignment="CENTER_LEFT" spacing="20.0">
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Площадь">
                        <font>
                            <Font size="16.0"/>
                        </font>
                    </Text>
                    <TextField fx:id="areaInput" onMouseClicked="#hideWarnings" promptText="0">
                        <cursor>
                            <Cursor fx:constant="TEXT"/>
                        </cursor>
                    </TextField>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="м²">
                        <font>
                            <Font size="13.0"/>
                        </font>
                    </Text>
                </HBox>
                <HBox alignment="CENTER_LEFT" spacing="20.0">
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Вместимость">
                        <font>
                            <Font size="16.0"/>
                        </font>
                    </Text>
                    <TextField fx:id="capacityInput" onMouseClicked="#hideWarnings" promptText="0">
                        <cursor>
                            <Cursor fx:constant="TEXT"/>
                        </cursor>
                    </TextField>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="мест">
                        <font>
                            <Font size="13.0"/>
                        </font>
                    </Text>
                </HBox>
                <HBox fx:id="eventProperty" alignment="CENTER_LEFT" prefHeight="124.0" prefWidth="478.0">
                    <VBox alignment="TOP_CENTER" prefHeight="67.0" prefWidth="183.0" spacing="23.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Кол-во одновременных мероприятий:"
                              wrappingWidth="182.6708984375">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Text>
                    </VBox>
                    <VBox prefHeight="124.0" prefWidth="294.0" spacing="10.0">
                        <RadioButton fx:id="onlyOneEvent" mnemonicParsing="false" onAction="#checkOnlyOneEvent"
                                     selected="true" text="Только одно">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </RadioButton>
                        <RadioButton fx:id="oneOrTwo" mnemonicParsing="false" onAction="#checkOneOrTwo"
                                     text="Одно или два">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </RadioButton>
                        <HBox alignment="CENTER_LEFT" prefHeight="25.0" prefWidth="279.0" spacing="10.0">
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                            <Text fx:id="areaText1" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: grey;"
                                  text="Площадь 1-ой части">
                                <font>
                                    <Font size="16.0"/>
                                </font>
                            </Text>
                            <TextField fx:id="firstArea" alignment="CENTER" disable="true" prefHeight="25.0"
                                       prefWidth="53.0" promptText="-1"/>
                            <Text fx:id="areaText2" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: grey;"
                                  text="м²">
                                <font>
                                    <Font size="16.0"/>
                                </font>
                            </Text>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0"
                              style="-fx-fill: grey;">
                            <padding>
                                <Insets right="10.0"/>
                            </padding>
                            <Text fx:id="areaText3" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: grey;"
                                  text="Площадь 2-ой части">
                                <font>
                                    <Font size="16.0"/>
                                </font>
                            </Text>
                            <TextField fx:id="secondArea" alignment="CENTER" disable="true" prefHeight="25.0"
                                       prefWidth="54.0" promptText="-1"/>
                            <Text fx:id="areaText4" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: grey;"
                                  text="м²">
                                <font>
                                    <Font size="16.0"/>
                                </font>
                            </Text>
                        </HBox>
                    </VBox>
                </HBox>
            </VBox>
        </VBox>
        <VBox alignment="TOP_RIGHT" prefHeight="452.0" prefWidth="708.0">
            <padding>
                <Insets bottom="5.0"/>
            </padding>
            <TableView fx:id="spacesTable" onMouseClicked="#hideWarnings" prefHeight="360.0" prefWidth="317.0">
                <columns>
                    <TableColumn fx:id="spaceColumn" prefWidth="89.0" text="Название"/>
                    <TableColumn fx:id="descriptionColumn" prefWidth="168.0" text="Описание"/>
                    <TableColumn fx:id="areaColumn" prefWidth="62.0" text="Площадь"/>
                    <TableColumn fx:id="capacityColumn" prefWidth="84.0" text="Вместимость"/>
                    <TableColumn fx:id="typeColumn" prefWidth="84.0" text="Тип событий"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <Text fx:id="warningSpace" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: red;"
                  text="Нужно выбрать помещение!" visible="false" wrappingWidth="160.13671875"/>
            <HBox alignment="CENTER_RIGHT" spacing="5.0">
                <Button mnemonicParsing="false" onAction="#edit" text="Изменить">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#delete" text="Удалить">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Button>
            </HBox>
        </VBox>
    </HBox>
    <HBox alignment="CENTER" prefHeight="0.0" prefWidth="960.0" spacing="300.0">
        <padding>
            <Insets bottom="5.0"/>
        </padding>
        <Button mnemonicParsing="false" onAction="#switchToPrimary" text="Назад">
            <font>
                <Font size="14.0"/>
            </font>
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Button>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="270.0" spacing="20.0">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <Text fx:id="successfulSaving" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: green;"
                  text="Сохранено!" visible="false"/>
            <Button mnemonicParsing="false" onAction="#save" text="Добавить">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#cleanForm" text="Очистить">
                <font>
                    <Font size="14.0"/>
                </font>
            </Button>
        </HBox>
    </HBox>
</VBox>
