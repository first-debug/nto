            <font>
               <Font size="18.0" />
            </font>
         </Button>
      </VBox>
   </HBox>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
      <Button mnemonicParsing="false" onAction="#toAddBooking" text="Забронировать помещение">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#toEditCoteries" text="Редактировать кружки">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </HBox>
